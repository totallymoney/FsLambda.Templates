{
  "author": "FsLambda",
  "classifications": ["AWS", "CDK", "Lambda", "F#"],
  "name": "F# AWS Lambda App",
  "groupIdentity": "FsLambda.App",
  "identity": "FsLambda.Blank",
  "shortName": "fslambda",
  "tags": { "language": "F#", "type": "project" },
  "sourceName": "NewApp",
  "primaryOutputs": [
    { "path": "src/NewApp/NewApp.fsproj" },
    { "path": "tests/NewApp.UnitTests/NewApp.UnitTests.fsproj" }
  ],
  "defaultName": "NewApp",
  "preferNameDirectory": true,
  "postActions": [],
  "sources": [
    {
      "source": "./",
      "target": "./",
      "include": ["**/*"],
      "exclude": [
        ".template.config/**/*",
        "[Bb]in/**/*",
        "[Oo]bj/**/*",
        "App.fs",
        "NewApp.fsproj",
        "NewApp.sln",
        "NewApp.slnx",
        "Platform/**/*",
        "cdk-ts/**/*",
        "cdk-fsharp/**/*"
      ]
    },
    {
      "condition": "(cdk == 'ts')",
      "source": "./cdk-ts/",
      "target": "./cdk/",
      "include": ["**/*"],
      "exclude": ["NewApp.sln", "NewApp.slnx"]
    },
    {
      "condition": "(cdk == 'ts' && framework == 'net8.0')",
      "source": "./cdk-ts/",
      "target": "./",
      "include": ["NewApp.sln"]
    },
    {
      "condition": "(cdk == 'ts' && framework == 'net10.0')",
      "source": "./cdk-ts/",
      "target": "./",
      "include": ["NewApp.slnx"]
    },
    {
      "condition": "(cdk == 'fsharp')",
      "source": "./cdk-fsharp/",
      "target": "./cdk/",
      "include": ["**/*"],
      "exclude": ["NewApp.sln", "NewApp.slnx"]
    },
    {
      "condition": "(cdk == 'fsharp' && framework == 'net8.0')",
      "source": "./cdk-fsharp/",
      "target": "./",
      "include": ["NewApp.sln"]
    },
    {
      "condition": "(cdk == 'fsharp' && framework == 'net10.0')",
      "source": "./cdk-fsharp/",
      "target": "./",
      "include": ["NewApp.slnx"]
    }
  ],
  "guids": [
    "0E9CB125-F04D-4E4A-8A20-7EB1FD9B43FB"
  ],
  "symbols": {
